{
  "parser": "@typescript-eslint/parser",
  "plugins": [
    "react-refresh",

    "@typescript-eslint"
  ],
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended",
    "plugin:react-hooks/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "react-refresh/only-export-components": "warn"
  },
  "env": {
    "browser": true,
    "es2020": true
  }
}
